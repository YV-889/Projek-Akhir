# .env di push ke repo sebagai testing
# .env.example dan .env.production dibuat dengan ai sebagai contoh konfigurasi


# Konfigurasi frotend
REACT_APP_API_BASE_URL=http://localhost:8080/api
REACT_APP_BACKEND_URL=http://localhost:8080
REACT_APP_ENVIRONMENT=development
REACT_APP_APP_NAME=Penjadwalan App
REACT_APP_VERSION=1.0.0

# Konfigurasi backend
DB_HOST=db
DB_NAME=penjadwalan_db
DB_USER=root
DB_PASSWORD=
DB_PORT=3306

# Konfigurasi mysql (gunakan docker/podman compose untuk mempermudah menjalankan db)
MYSQL_ROOT_PASSWORD=
MYSQL_ALLOW_EMPTY_PASSWORD=1
MYSQL_DATABASE=penjadwalan_db

# Port development (akan berubah di production)
BACKEND_PORT=8080
FRONTEND_PORT=3000
MYSQL_PORT=3306
PHPMYADMIN_PORT=8081

# Konfigurasi external api (tambahkan disini untuk fitur tambahan)
HOLIDAY_API_URL=https://api-harilibur.vercel.app/api
HOLIDAY_API_TIMEOUT=5000

# Setting aplikasi
APP_DEBUG=true
APP_TIMEZONE=Asia/Jakarta
APP_LOCALE=id

# Konfig env podman/docker
COMPOSE_PROJECT_NAME=penjadwalan-app